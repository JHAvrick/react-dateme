{
  "name": "react-dateme",
  "version": "1.0.1",
  "description": "React date/time component",
  "main": "build/react-dateme.js",
  "scripts": {
    "build": "webpack -p",
    "build-style": "node-sass ./src/sass/all.scss ./build/react-dateme.css --output-style compressed",
    "dev": "webpack -d --watch",
    "server": "node ./test/server.js",
    "style-dev": "node-sass --watch ./src/sass/all.scss ./build/react-dateme.css",
    "style-main": "node-sass --watch ./src/sass/all.scss ./test/app/style/components/react-dateme.scss",
    "style-test": "node-sass --watch ./test/app/style/main.scss ./test/index.css"
  },
  "author": "Joshua Avrick",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "express": "^4.15.4",
    "ip": "^1.1.5",
    "node-sass": "^4.5.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.4.1",
    "webpack-bundle-analyzer": "^2.9.0"
  },
  "dependencies": {
    "react-onclickout": "^2.0.7"
  }
}
